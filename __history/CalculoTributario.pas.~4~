  System.SysUtils, System.Classes;

type TCSts         = ( p000, p010, p020, p030, p040, p050, p051, p060, p070, p090, p101, p102, p103, p201, p202, p203, p300, p400, p500, p900 );
type TCstPisCofins = ( pis01 ,pis02, pis03, pis04, pis05, pis06, pis07, pis08, pis09, pis49, pis50, pis51, pis52, pis53, pis54, pis55, pis56, pis60, pis61, pis62, pis63, pis64, pis65, pis66, pis67, pis70, pis71, pis72, pis73, pis74, pis75, pis98, pis99 );
type TTipoPessoa   = ( pFisica, pJuridica );
type TTpNF         = ( tpSaida, tpEntrada );
type TpModalidade  = ( tmValorOperacao, tmPauta, tmMargemValorAgregado );

type
  TDTTributo = class(TComponent)
  private

  protected

  public

  published

  end;

procedure Register;

implementation

